{
    "name": "HackemUp",
    "version": "1.2",
    "description": "Keep track of what's changed on Hacker News front page since the last time you looked",
    "manifest_version": 2,
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "content_scripts": [
        {
            "matches": [
                "*://news.ycombinator.com/",
                "*://news.ycombinator.com/news"
            ],
            "js": [
                "zepto.min.js",
                "init.js"
            ]
        }
    ]
}
